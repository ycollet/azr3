@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix ll: <http://ll-plugins.nongnu.org/lv2/namespace#>.

<http://ll-plugins.nongnu.org/lv2/dev/mrvalve/0.0.0>
  a lv2:Plugin;
  doap:name "Mr Valve";
  doap:license <http://usefulinc.com/doap/licenses/gpl>;
  ll:pegName "v";
  	
  lv2:port [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 0;
    lv2:minimum 0;
    lv2:default 0.5;
    lv2:maximum 1;
    lv2:symbol "drive";
    lv2:name "Drive";
  ],
  
  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 1;
    lv2:minimum 0;
    lv2:default 0.03;
    lv2:maximum 1;
    lv2:symbol "set";
    lv2:name "Set";
  ],
  
  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 2;
    lv2:minimum 0;
    lv2:default 0.8;
    lv2:maximum 1;
    lv2:symbol "tone";
    lv2:name "Tone";
  ],
  
  [
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 3;
    lv2:minimum 0;
    lv2:default 1;
    lv2:maximum 1;
    lv2:symbol "mix";
    lv2:name "Mix";
  ],
  
  [
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 4;
    lv2:symbol "input";
    lv2:name "Input";
  ],
  
  [
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 5;
    lv2:symbol "output";
    lv2:name "Output";
  ].
